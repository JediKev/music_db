<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @song.name %>
</p>

<p>
  <strong>Artist:</strong>
  <%= @song.artist.name %>
</p>

<p>
	<strong>Genre:</strong>
	<ul>
		<% @artist.genre.order('name').each do |genre| %>
		<li><%= genre.name %></li>
		<% end %>
	</ul>
</p>

<%= link_to 'Edit', edit_song_path(@song) %> |
<%= link_to 'Back', songs_path %>
